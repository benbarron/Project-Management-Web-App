    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "bootstrap": "^4.3.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
