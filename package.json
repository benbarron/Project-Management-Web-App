    "express": "^4.17.1",
    "express-edge": "^1.0.0",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.6.1",
    "nodemailer": "^6.3.0",
    "path": "^0.12.7",
    "redis": "^2.8.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
